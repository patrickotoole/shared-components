"/var/log/nginx/*.log" {
  rotate 7
  create 0644 www-data adm
  missingok
  delaycompress
  notifempty
  compress
  sharedscripts
  postrotate
    kill -USR1 $(cat /var/run/nginx.pid); kill -HUP $(cat /var/run/supervisord.pid);
  endscript
}
